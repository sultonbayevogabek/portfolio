<%
let {
    profileImg,
    jobs,
    about,
    email,
    phone,
    telegram,
    github,
    youtube,
    education,
    experience,
    skills,
    projects
} = data
%>
<!DOCTYPE html>
<html lang="en">
<head>
    <title><%= title %></title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="https://fonts.googleapis.com/css?family=Poppins:100,200,300,400,500,600,700,800,900" rel="stylesheet">
    <link rel="stylesheet" href="/css/styles.css">
    <style>
       .scrollax-performance *, .scrollax-performance *:before, .scrollax-performance *:after {
          pointer-events: none !important;
          -webkit-animation-play-state: paused !important;
          animation-play-state: paused !important;
       }

       .txt-rotate > .wrap {
          border-right: 0.08em solid #666
       }
    </style>
</head>

<body data-spy="scroll" data-target=".site-navbar-target" data-offset="300" data-aos-easing="slide"
      data-aos-duration="800" data-aos-delay="0">
<nav class="navbar navbar-expand-lg navbar-dark ftco_navbar ftco-navbar-light site-navbar-target" id="ftco-navbar">
    <div class="container">
        <a class="navbar-brand" href="/"><span>O</span>ƒùabek</a>
        <button class="navbar-toggler js-fh5co-nav-toggle fh5co-nav-toggle" type="button" data-toggle="collapse"
                data-target="#ftco-nav" aria-controls="ftco-nav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="oi oi-menu"></span> Menu
        </button>
        <div class="collapse navbar-collapse" id="ftco-nav">
            <ul class="navbar-nav nav ml-auto">
                <li class="nav-item"><a href="#" class="nav-link"><span>Home</span></a></li>
                <li class="nav-item"><a href="#about-section" class="nav-link"><span>About</span></a></li>
                <li class="nav-item"><a href="#resume-section" class="nav-link"><span>Resume</span></a></li>
                <li class="nav-item"><a href="#projects-section" class="nav-link"><span>Projects</span></a></li>
                <li class="nav-item"><a href="#contact-section" class="nav-link"><span>Contact</span></a></li>
            </ul>
        </div>
    </div>
</nav>

<section class="hero-wrap js-fullheight" style="height: 200px;">
    <div class="overlay"></div>
    <div class="container">
        <div class="row no-gutters slider-text js-fullheight justify-content-center align-items-center"
             style="height: 200px;">
            <div class="col-lg-8 col-md-6 ftco-animate d-flex align-items-center fadeInUp ftco-animated">
                <div class="text text-center">
                    <span class="subheading">Hey! I am</span>
                    <h1>Og'abek Sultonbayev</h1>
                    <% let jobsArray = jobs.map(job => {
                        return ` ${job}.`
                    })%>
                    <h2>I'm a
                        <span class="txt-rotate" data-period="2000"
                              data-rotate="<%= JSON.stringify(jobsArray)%>"><span
                                    class="wrap"></span></span>
                    </h2>
                </div>
            </div>
        </div>
    </div>

    <div class="mouse">
        <a href="#" class="mouse-icon">
            <div class="mouse-wheel"><span class="ion-ios-arrow-round-down"></span></div>
        </a>
    </div>
</section>

<section class="ftco-about img ftco-section ftco-no-pt ftco-no-pb" id="about-section">
    <div class="container">
        <div class="row d-flex no-gutters">
            <div class="col-md-6 col-lg-6 d-flex">
                <div class="img-about img d-flex align-items-stretch">
                    <div class="overlay"></div>
                    <div class="img d-flex align-self-stretch align-items-center"
                         style="background-image:url(<%= profileImg %>)">
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-lg-6 pl-md-5 py-5">
                <div class="row justify-content-start pb-3">
                    <div class="col-md-12 heading-section ftco-animate">
                        <h1 class="big">About</h1>
                        <h2 class="mb-4">About Me</h2>
                        <p><%= about.section_description %></p>
                        <ul class="about-info mt-4 px-md-0 px-2">
                            <li class="d-flex"><span>Name:</span> <span>Og'abek Sultonbayev</span></li>
                            <li class="d-flex"><span>Date of birth:</span> <span>October 31, 1999</span></li>
                            <li class="d-flex"><span>Address:</span> <span>Khorezm, Uzbekistan</span></li>
                            <li class="d-flex"><span>Zip code:</span> <span>220100</span></li>
                            <li class="d-flex"><span>Email:</span> <span><%= email %></span></li>
                            <li class="d-flex"><span>Phone: </span> <span><%= phone %></span></li>
                        </ul>
                    </div>
                </div>
                <div class="counter-wrap ftco-animate d-flex mt-md-3">
                    <div class="text">
                        <p class="mb-4">
                            <span class="number"><%= projects.length %></span>
                            <span>Project complete</span>
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="ftco-section ftco-no-pb goto-here pt-5" id="resume-section">
    <div class="container">
        <div class="row">
            <div class="col-md-3">
                <nav id="navi">
                    <ul>
                        <li><a href="#page-1">Education</a></li>
                        <li><a href="#page-2">Experience</a></li>
                        <li><a href="#page-3">Skills</a></li>
                    </ul>
                </nav>
            </div>
            <div class="col-md-9">
                <div id="page-1" class="page one">
                    <h2 class="heading">Education</h2>
                    <% education.forEach(edu => { %>
                        <div class="resume-wrap d-flex ftco-animate">
                        <div class="icon d-flex align-items-center justify-content-center">
                            <span class="flaticon-ideas"></span>
                        </div>
                        <div class="text pl-3">
                            <span class="date"><%= edu.education_years %></span>
                            <h2><%= edu.education_field %></h2>
                            <span class="position"><%= edu.education_place %></span>
                            <p><%= edu.education_description %></p>
                        </div>
                    </div>
                    <% }) %>
                </div>
                <div id="page-2" class="page two">
                    <h2 class="heading">Experience</h2>
                    <% experience.forEach(exp => { %>
                        <div class="resume-wrap d-flex ftco-animate">
                            <div class="icon d-flex align-items-center justify-content-center">
                                <span class="flaticon-ideas"></span>
                            </div>
                            <div class="text pl-3">
                                <span class="date"><%= exp.experience_years %></span>
                                <h2><%= exp.experience_field %></h2>
                                <span class="position"><%= exp.experience_place %></span>
                                <p><%= exp.experience_description %></p>
                            </div>
                        </div>
                    <% }) %>
                </div>
                <div id="page-3" class="page three">
                    <h2 class="heading">Skills</h2>

                    <div class="row">
                        <% skills = skills.sort((a, b) => {
                            return a.skill_order - b.skill_order
                        })%>
                        <% skills.forEach(skill =>{ %>
                        <div class="col-md-6 animate-box">
                            <div class="progress-wrap ftco-animate">
                                <h3><%= skill.skill_name %></h3>
                                <div class="progress">
                                    <div class="progress-bar color-1" role="progressbar" aria-valuenow="<%= skill.skill_degree %>"
                                         aria-valuemin="0"
                                         aria-valuemax="100" style="width: <%= skill.skill_degree %>%">
                                        <span><%= skill.skill_degree %>%</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <% }) %>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="ftco-section ftco-project" id="projects-section">
    <div class="container px-md-0">
        <div class="row no-gutters justify-content-center pb-5">
            <div class="col-md-12 heading-section text-center ftco-animate">
                <h1 class="big big-2">Projects</h1>
                <h2 class="mb-4">My Projects</h2>
                <p>A list of things I've done to date</p>
            </div>
        </div>
        <div class="row no-gutters">
            <% projects.forEach(project => { %>
                <div class="col-md-4">
                <div class="project img ftco-animate d-flex justify-content-center align-items-center"
                     style="background-image: url(<%= project.project_poster %>); background-size: 100%; background-repeat: no-repeat; background-position: center">
                    <div class="overlay"></div>
                    <div class="text text-center p-4">
                        <h3><a target="_blank" href="<%= project.project_link %>"><%= project.project_title %></a></h3>
                        <span><%= project.project_type %></span>
                    </div>
                </div>
            </div>
            <% }) %>
        </div>
    </div>
</section>

<section class="ftco-section contact-section ftco-no-pb" id="contact-section">
    <div class="container">
        <div class="row justify-content-center mb-5 pb-3">
            <div class="col-md-7 heading-section text-center ftco-animate">
                <h1 class="big big-2">Contact</h1>
                <h2 class="mb-4">Contact Me</h2>
                <p>My contact information to contact me</p>
            </div>
        </div>
        <div class="row d-flex contact-info mb-5">
            <div class="col-md-6 col-lg-4 d-flex ftco-animate">
                <div class="align-self-stretch box text-center p-4 shadow">
                    <div class="icon d-flex align-items-center justify-content-center">
                        <span class="icon-phone2"></span>
                    </div>
                    <div>
                        <h3 class="mb-4">Phone Number</h3>
                        <p><a href="tel:<%= phone.replace(/ /g, '')%>"><%= phone %></a></p>
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-lg-4 d-flex ftco-animate">
                <div class="align-self-stretch box text-center p-4 shadow">
                    <div class="icon d-flex align-items-center justify-content-center">
                        <span class="icon-mail"></span>
                    </div>
                    <div>
                        <h3 class="mb-4">Email Address</h3>
                        <p><a target="_blank" href="mailto: <%= email %>"><%= email %></a>
                        </p>
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-lg-4 d-flex ftco-animate">
                <div class="align-self-stretch box text-center p-4 shadow">
                    <div class="icon d-flex align-items-center justify-content-center">
                        <span class="icon-paper-plane"></span>
                    </div>
                    <div>
                        <h3 class="mb-4">My Telegram</h3>
                        <p><a target="_blank" href="https://t.me/<%= telegram.substring(1) %>"><%= telegram %></a></p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<footer class="ftco-footer ftco-section">
    <div class="container">
        <div class="row">
            <div class="col-md">
                <div class="ftco-footer-widget mb-4">
                    <h2 class="ftco-heading-2">About</h2>
                    <p><%= about.section_description %></p>
                </div>
            </div>
            <div class="col-md">
                <div class="ftco-footer-widget mb-4 ml-md-4">
                    <h2 class="ftco-heading-2">Links</h2>
                    <ul class="list-unstyled">
                        <li><a href="#"><span class="icon-long-arrow-right mr-2"></span>Home</a></li>
                        <li><a href="#about-section"><span class="icon-long-arrow-right mr-2"></span>About</a></li>
                        <li><a href="#resume-section"><span class="icon-long-arrow-right mr-2"></span>Resume</a></li>
                        <li><a href="#projects-section"><span class="icon-long-arrow-right mr-2"></span>Projects</a>
                        </li>
                        <li><a href="#contact-section"><span class="icon-long-arrow-right mr-2"></span>Contact</a></li>
                    </ul>
                </div>
            </div>
            <div class="col-md">
                <div class="ftco-footer-widget mb-4">
                    <h2 class="ftco-heading-2">Info</h2>
                    <div class="block-23 mb-3">
                        <ul>
                            <li>
                                <a target="_blank" href="https://goo.gl/maps/3FwbYJpvovXCEdxx9">
                                    <span class="icon icon-map-marker"></span>
                                    <span class="text">Uzbekistan, Khorezm region, Tuprokkala district</span>
                                </a>
                            </li>
                            <li>
                                <a href="tel: <%= phone.replace(/ /g, '') %>">
                                    <span class="icon icon-phone"></span>
                                    <span class="text"><%= phone %></span>
                                </a>
                            </li>
                            <li>
                                <a target="_blank" href="mailto: <%= email %>">
                                    <span class="icon icon-envelope"></span>
                                    <span class="text"><%= email.substring(0, email.length - 10) %></span>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="col-md">
                <div class="ftco-footer-widget mb-4 ml-md-4">
                    <h2 class="ftco-heading-2">Socials</h2>
                    <ul class="list-unstyled">
                        <li>
                            <a target="_blank" href="https://t.me/<%= telegram.substring(1) %>">Telegram</a>
                        </li>
                        <li>
                            <a target="_blank" href="<%= github %>">Github</a>
                        </li>
                        <li>
                            <a target="_blank" href="mailto:<%= email %>">Email</a>
                        </li>
                        <li>
                            <a target="_blank"
                               href="<%= youtube %>">YouTube</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</footer>

